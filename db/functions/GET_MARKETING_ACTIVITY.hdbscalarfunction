FUNCTION "MKTG_PLANNING_TOOL"."mktgplanningtool.db.functions::GET_MARKETING_ACTIVITY"
     (IN HL5_ID BIGINT )
    	RETURNS PATH NVARCHAR(100)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	AS
BEGIN

DECLARE VA_MARKETING_ACTIVITY NVARCHAR(100);
VA_MARKETING_ACTIVITY := '';

CALL "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_MARKETING_ACTIVITY_BY_HL5_ID"(HL5_ID, VA_MARKETING_ACTIVITY);

select :VA_MARKETING_ACTIVITY into PATH from dummy;

END;
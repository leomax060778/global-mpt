PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_SEGMENTATION_DEPARTMENT"(
		OUT out_result TABLE (
			SEGMENTATION_DEPARTMENT_ID BIGINT,
			NAME NVARCHAR(255),
			PARENT_ID BIGINT
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT SEGMENTATION_DEPARTMENT_ID,
				NAME, PARENT_ID
			FROM SEGMENTATION_DEPARTMENT
			WHERE ENABLED = 1
				AND DELETED = 0
			ORDER BY NAME ASC;
END;

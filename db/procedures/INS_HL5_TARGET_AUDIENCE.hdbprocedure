PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::INS_HL5_TARGET_AUDIENCE"
(
	IN target_audience TABLE(
	    in_hl5_id BIGINT,
        in_target_audience_id BIGINT,
        in_created_user_id BIGINT
	),
	OUT out_result BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN

	INSERT INTO HL5_TARGET_AUDIENCE	(
	    hl5_id,
        target_audience_id,
	    created_user_id
	)
	(SELECT in_hl5_id,
            in_target_audience_id,
            in_created_user_id
     FROM :target_audience);
	
	SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO out_result FROM "MKTG_PLANNING_TOOL"."HL5_TARGET_AUDIENCE";
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_EXPECTED_OUTCOMES_BY_HL3_ID" (
		IN in_hl3_id bigint,
		OUT out_expected_outcomes TABLE(
			hl3_expected_outcomes_id BIGINT,
			comments NVARCHAR(100)
		)
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	out_expected_outcomes = SELECT hl3_expected_outcomes_id, comments from HL3_EXPECTED_OUTCOMES
					where deleted = 0 and enabled = 1 and hl3_id = in_hl3_id;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_SEGMENTATION_SALES_BY_PARENT_ID"
(
    IN IN_PARENT_ID BIGINT,
    OUT out_result TABLE(
          SEGMENTATION_SALES_ID BIGINT
        , NAME NVARCHAR(255)
        , PARENT_ID BIGINT
    )
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN

out_result =
	select
	 SEGMENTATION_SALES_ID
	,NAME
	,PARENT_ID
	FROM SEGMENTATION_SALES
	WHERE
	PARENT_ID = IN_PARENT_ID
	AND DELETED = 0;
END;

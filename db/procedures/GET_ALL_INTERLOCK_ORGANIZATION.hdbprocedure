PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_INTERLOCK_ORGANIZATION" ( 
OUT out_result TABLE(
	INTERLOCK_ORGANIZATION_ID bigint,
	NAME nvarchar(255)
	)
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT	INTERLOCK_ORGANIZATION_ID,
						NAME
				 FROM INTERLOCK_ORGANIZATION
				 	WHERE   ENABLED = 1 
				 		AND DELETED = 0
				 ORDER BY NAME ASC;
	
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_SEGMENTATION_TACTIC_BY_ID"
(
    IN in_segmentation_tactic_id BIGINT,
    OUT out_result TABLE(
          SEGMENTATION_TACTIC_ID BIGINT
        , NAME NVARCHAR(255)
    )
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN

out_result =
	select
	 SEGMENTATION_TACTIC_ID
	,NAME
	FROM SEGMENTATION_TACTIC
	WHERE
	SEGMENTATION_TACTIC_ID = in_segmentation_tactic_id;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_COUNT_OUTCOMES_BY_OUTCOMES_TYPE_ID"(
		IN in_outcomes_type_id BIGINT, 
		OUT out_result BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	SELECT COUNT(outcomes_id)
		INTO out_result
		FROM outcomes
		WHERE outcomes_type_id = in_outcomes_type_id
			AND enabled = 1
			AND deleted = 0;
END;

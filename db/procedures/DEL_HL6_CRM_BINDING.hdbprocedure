PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL6_CRM_BINDING"(
		IN in_hl6_id BIGINT, 
		OUT out_result INTEGER
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM "MKTG_PLANNING_TOOL"."HL6_CRM_BINDING" AS hcb
	WHERE hcb.hl6_id = in_hl6_id;
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL2_EVENT_APPROVER" (
	IN IN_HL2_ID BIGINT
	,OUT out_result INTEGER
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN
	
	DELETE FROM HL2_EVENT_APPROVER
	WHERE HL2_ID = IN_HL2_ID;
	
	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;

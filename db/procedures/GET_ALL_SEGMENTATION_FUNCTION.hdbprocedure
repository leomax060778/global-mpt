PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_SEGMENTATION_FUNCTION"(
		OUT out_result TABLE (
			SEGMENTATION_FUNCTION_ID BIGINT,
			NAME NVARCHAR(255),
			PARENT_ID BIGINT
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT SEGMENTATION_FUNCTION_ID, 
				NAME, 
				PARENT_ID
			FROM SEGMENTATION_FUNCTION
			WHERE ENABLED = 1
				AND DELETED = 0
			ORDER BY NAME ASC;
END;

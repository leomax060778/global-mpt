PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::INS_ALLOCATION_CATEGORY" (
	IN in_description NVARCHAR(255),
	IN in_name NVARCHAR(255),
	IN in_measure_id BIGINT,
	IN in_single_option_only TINYINT,
	IN in_user_id BIGINT,
	IN in_category_type_id BIGINT,
	OUT out_category_id BIGINT
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	AS
BEGIN
		INSERT INTO "MKTG_PLANNING_TOOL"."ALLOCATION_CATEGORY"(measure_id, name, description, single_option_only, created_user_id, category_type_id)
		VALUES(in_measure_id, in_name, in_description, in_single_option_only, in_user_id, in_category_type_id);
		
		SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO out_category_id FROM "MKTG_PLANNING_TOOL"."ALLOCATION_CATEGORY";
END;
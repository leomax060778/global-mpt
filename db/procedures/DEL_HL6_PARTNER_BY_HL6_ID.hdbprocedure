PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL6_PARTNER_BY_HL6_ID"
(
	in in_hl6_id bigint,
	in in_modified_user_id bigint,
	out out_result bigint
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN

	UPDATE HL6_PARTNER
	SET
	deleted = 1,
	enabled = 0,
	modified_user_id = in_modified_user_id,
	modified_date_tz = CURRENT_TIMESTAMP
	where hl6_id = in_hl6_id;
	
	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;
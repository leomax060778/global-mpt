PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_CATEGORY_TYPE" (
	OUT out_result TABLE (
	category_type_id BIGINT
	, name NVARCHAR(60)
	)
 	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

		out_result = SELECT allocation_category_type_id as category_type_id,
			   name
		FROM ALLOCATION_CATEGORY_TYPE
		WHERE enabled = 1 AND deleted = 0
		ORDER BY upper(name);

END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL5_TARGET_AUDIENCE_BY_HL5_ID"
(
	IN in_hl5_id BIGINT,
	OUT out_result BIGINT
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN
	DELETE FROM HL5_TARGET_AUDIENCE
	WHERE HL5_ID = in_hl5_id;
	
	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;

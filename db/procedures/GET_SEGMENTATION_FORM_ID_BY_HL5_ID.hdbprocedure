PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_SEGMENTATION_FORM_ID_BY_HL5_ID" (
    IN in_hl5_id BIGINT,
	OUT out_result TABLE (
	SEGMENTATION_FORM_ID BIGINT
	)
 	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

		out_result = SELECT SEGMENTATION_FORM_ID
		FROM SEGMENTATION_FORM
		WHERE HL5_ID = in_hl5_id;

END;

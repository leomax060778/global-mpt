PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_SEGMENTATION_TACTIC"(
		OUT out_result TABLE (
			SEGMENTATION_TACTIC_ID BIGINT,
			NAME NVARCHAR(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT SEGMENTATION_TACTIC_ID,
				NAME
			FROM SEGMENTATION_TACTIC
			WHERE ENABLED = 1
				AND DELETED = 0
			ORDER BY NAME ASC;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_ROLE"(
		OUT out_result TABLE (
			ROLE_ID BIGINT,
			NAME NVARCHAR(25)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT ROLE_ID, 
				NAME
			FROM "ROLE"
			WHERE ENABLED = 1
				AND DELETED = 0;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::INS_SALE_ORGANIZATION" (
	IN IN_NAME NVARCHAR(255),
	IN IN_CRM_KEY NVARCHAR(15),
	IN IN_CREATED_USER_ID bigint,
	OUT OUT_RESULT bigint
 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN
/***************************** 
	Write your procedure logic 
 *****************************/
 
	INSERT INTO "MKTG_PLANNING_TOOL"."SALE_ORGANIZATION"(NAME, CRM_KEY ,created_user_id)
	VALUES(	IN_NAME , IN_CRM_KEY ,IN_CREATED_USER_ID);
	
	SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO OUT_RESULT FROM "MKTG_PLANNING_TOOL"."SALE_ORGANIZATION";
 
END;
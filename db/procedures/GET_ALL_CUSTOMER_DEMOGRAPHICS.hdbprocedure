PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_CUSTOMER_DEMOGRAPHICS" (
	OUT out_result TABLE(
		CUSTOMER_DEMOGRAPHICS_ID INTEGER,
		NAME NVARCHAR(10)
	)
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	
	out_result =
				SELECT 	CUSTOMER_DEMOGRAPHICS_ID,
						NAME
					FROM CUSTOMER_DEMOGRAPHICS
					WHERE ENABLED = 1
						AND DELETED = 0;	
END;

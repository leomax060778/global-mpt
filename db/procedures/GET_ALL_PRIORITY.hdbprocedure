PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_PRIORITY" (
		OUT out_result TABLE (PRIORITY_ID INTEGER, NAME NVARCHAR(255), CRM_KEY NVARCHAR(255))
	 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT PRIORITY_ID
						, NAME
						, CRM_KEY
				FROM PRIORITY
				WHERE PRIORITY.ENABLED = 1 AND PRIORITY.DELETED = 0;
END;

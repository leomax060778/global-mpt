PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::UPD_HL4_BUDGET" (
	IN in_hl4_id bigint,
	IN in_user_id bigint,
	IN in_updated_budget DECIMAL(19, 6),
	OUT out_result bigint
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN
	UPDATE HL4 SET
	HL4.modified_date_tz = CURRENT_TIMESTAMP
    ,HL4.modified_user_id = in_user_id
    ,HL4.budget = in_updated_budget
	WHERE HL4.hl4_id = in_hl4_id;

	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;

END;

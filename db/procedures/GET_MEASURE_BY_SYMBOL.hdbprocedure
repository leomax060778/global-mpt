PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_MEASURE_BY_SYMBOL" (
    in in_symbol NVARCHAR (255)
	,OUT out_result TABLE(measure_id BIGINT, measure_name NVARCHAR (255), symbol NVARCHAR (255))
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
		out_result = SELECT measure_id,
			   measure_name, symbol
		FROM measure
		WHERE deleted = 0
		  AND enabled = 1
		  AND upper(symbol) = upper(in_symbol);
END;

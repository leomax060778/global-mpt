PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_HL5_BY_ACRONYM"(
		IN in_hl5_acronym NVARCHAR(25), 
		IN in_hl4_id BIGINT, 
		OUT out_result TABLE (
			hl5_id BIGINT
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT hl5_id
			FROM HL5
			WHERE upper(acronym) = upper(in_hl5_acronym)
				AND HL5.hl4_id = in_hl4_id
				AND ENABLED = 1
				AND DELETED = 0;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_PARTNER_TYPE"(
		OUT out_partner_type TABLE (
			PARTNER_TYPE_ID BIGINT,
			TYPE_NAME NVARCHAR(100)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	out_partner_type = SELECT PARTNER_TYPE_ID, 
				TYPE_NAME
			FROM PARTNER_TYPE
			WHERE enabled = 1
				AND deleted = 0;
END;

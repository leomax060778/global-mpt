PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_HL6_BY_IMPORT_ID"(
		IN in_import_id BIGINT, 
		OUT out_result TABLE (
			hl6_id INTEGER
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT hl6_id
			FROM hl6
			WHERE import_id = in_import_id;
END;

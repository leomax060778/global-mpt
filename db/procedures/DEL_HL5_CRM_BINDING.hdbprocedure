PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL5_CRM_BINDING"(
		IN in_hl5_id BIGINT, 
		OUT out_result INTEGER
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM "MKTG_PLANNING_TOOL"."HL5_CRM_BINDING" AS hcb
	WHERE hcb.hl5_id = in_hl5_id;
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_DISTRIBUTION_CHANNEL"(
		OUT out_result TABLE (
			DISTRIBUTION_CHANNEL_ID BIGINT,
			NAME NVARCHAR(255),
			CRM_KEY NVARCHAR(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT DISTRIBUTION_CHANNEL_ID, 
				NAME, 
				CRM_KEY
			FROM "DISTRIBUTION_CHANNEL"
			WHERE ENABLED = 1
				AND DELETED = 0
			ORDER BY NAME ASC;
END;

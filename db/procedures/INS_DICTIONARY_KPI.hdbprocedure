PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::INS_DICTIONARY_KPI" (
	IN IN_PATH NVARCHAR(255),
    IN IN_KEY NVARCHAR(255),
    IN IN_VALUE NVARCHAR(2000),
    IN HIERARCHY_LEVEL_ID TINYINT,
    IN IN_USER_ID BIGINT,

	OUT OUT_RESULT INTEGER
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	AS
BEGIN

    --DELETE FROM UPLOAD_DICTIONARY WHERE PATH = IN_PATH AND UPLOAD_KEY = IN_KEY;
    INSERT INTO UPLOAD_DICTIONARY(
     PATH ,
     UPLOAD_KEY ,
     UPLOAD_VALUE ,
     HIERARCHY_LEVEL_ID ,
     created_user_id
    )
    VALUES (
    IN_PATH,
    IN_KEY ,
    IN_VALUE ,
    HIERARCHY_LEVEL_ID,
    IN_USER_ID
    );

	SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO OUT_RESULT FROM "MKTG_PLANNING_TOOL"."UPLOAD_DICTIONARY";
END;

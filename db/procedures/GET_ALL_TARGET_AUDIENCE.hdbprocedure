PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_TARGET_AUDIENCE" (
    OUT out_result TABLE(
        TARGET_AUDIENCE_ID BIGINT,
        NAME NVARCHAR(255)
    )
)

LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
READS SQL DATA AS
BEGIN
    out_result = SELECT TARGET_AUDIENCE_ID,
                        NAME
	             FROM "TARGET_AUDIENCE"
 	             WHERE  ENABLED = 1
 	                AND DELETED = 0;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL5_CATEGORY_OPTION_HARD"(
		IN in_hl5_id BIGINT, 
		OUT out_result BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM hl5_category_option
	WHERE hl5_id = in_hl5_id;
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

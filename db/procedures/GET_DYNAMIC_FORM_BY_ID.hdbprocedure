PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_DYNAMIC_FORM_BY_ID" (
    IN in_dynamic_form_id bigint,
	OUT out_result TABLE(
	
		DYNAMIC_FORM_ID BIGINT,
        DESCRIPTION NVARCHAR(255)
	)
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

out_result = select df.DYNAMIC_FORM_ID, df.DESCRIPTION
    from DYNAMIC_FORM df
    where df.DYNAMIC_FORM_ID = in_dynamic_form_id
    and df.enabled = 1 and df.deleted = 0;

END;

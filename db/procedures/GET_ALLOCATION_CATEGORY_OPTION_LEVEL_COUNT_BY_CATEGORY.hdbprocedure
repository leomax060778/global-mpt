PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALLOCATION_CATEGORY_OPTION_LEVEL_COUNT_BY_CATEGORY"(
		IN in_category_id BIGINT, 
		OUT out_result INTEGER
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	SELECT COALESCE(
				COUNT(allocation_category_option_level_id), 
				0
			)
		INTO out_result
		FROM allocation_category_option_level AS ACOL
		WHERE ACOL.allocation_category_id = in_category_id;
END;

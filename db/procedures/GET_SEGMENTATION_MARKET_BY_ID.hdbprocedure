PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_SEGMENTATION_MARKET_BY_ID"
(
    IN in_segmentation_market_id BIGINT,
    OUT out_result TABLE(
          SEGMENTATION_MARKET_ID BIGINT
        , NAME NVARCHAR(255)
    )
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN

out_result =
	select
	 SEGMENTATION_MARKET_ID
	,NAME
	FROM SEGMENTATION_MARKET
	WHERE
	SEGMENTATION_MARKET_ID = in_segmentation_market_id;
END;

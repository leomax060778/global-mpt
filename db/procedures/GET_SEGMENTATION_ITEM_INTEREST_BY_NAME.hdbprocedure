PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_SEGMENTATION_ITEM_INTEREST_BY_NAME"
(
        IN IN_NAME NVARCHAR(255),
        OUT out_result TABLE(
              SEGMENTATION_ITEM_INTEREST_ID BIGINT
            , NAME NVARCHAR(255)
        )
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN

out_result =
	select
	 SEGMENTATION_ITEM_INTEREST_ID
	,NAME
	FROM SEGMENTATION_ITEM_INTEREST
	WHERE
	upper(name) = upper(IN_NAME)
	and deleted = 0 and enabled = 1;
END;

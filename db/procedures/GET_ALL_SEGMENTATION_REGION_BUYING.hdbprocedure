PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_SEGMENTATION_REGION_BUYING"(
		OUT out_result TABLE (
			SEGMENTATION_REGION_BUYING_ID BIGINT,
			NAME NVARCHAR(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT SEGMENTATION_REGION_BUYING_ID,
				NAME
			FROM SEGMENTATION_REGION_BUYING
			WHERE ENABLED = 1
				AND DELETED = 0
			ORDER BY NAME ASC;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_L2_BUDGET_SPEND_APPROVER"(
		IN in_l2_id BIGINT, 
		IN in_budget_approver_id TABLE (
			in_user_id BIGINT
		), 
		OUT out_result BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM hl2_budget_spend_approver
	WHERE hl2_id = in_l2_id;
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

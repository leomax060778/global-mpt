PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_CRM_PARENT_PATH"(
		IN in_level_id BIGINT, 
		IN in_id BIGINT, 
		OUT out_result BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM CRM_PARENT_PATH
	WHERE LEVEL_ID = in_id
		AND HIERARCHY_LEVEL_ID = in_level_id;
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

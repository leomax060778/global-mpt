PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::UPD_REGION_GLOBAL_FLAG" (
	IN IN_USER_ID BIGINT,
	IN IN_IS_GLOBAL_REGION TINYINT,
	OUT out_result BIGINT
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN

	UPDATE REGION
	SET
	MODIFIED_USER_ID = IN_USER_ID,
	MODIFIED_DATE_TZ = CURRENT_TIMESTAMP,
	IS_GLOBAL_REGION = IN_IS_GLOBAL_REGION;

	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;


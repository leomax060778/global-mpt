PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_HL3_USER"(
		IN in_hl3_id BIGINT, 
		IN in_user_id BIGINT, 
		OUT out_result TABLE (
			hl3_user_id BIGINT,
			hl3_id BIGINT,
			user_id BIGINT
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT hl3_user_id, 
				hl3_id, 
				user_id
			FROM HL3_USER
			WHERE in_hl3_id = hl3_id
				AND user_id = in_user_id;
END;

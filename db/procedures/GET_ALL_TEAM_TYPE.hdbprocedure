PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_TEAM_TYPE" (
	OUT OUT_RESULT TABLE(
		TEAM_TYPE_ID BIGINT,
		TEAM_TYPE_NAME NVARCHAR(255)
	)
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

	OUT_RESULT = SELECT TT.TEAM_TYPE_ID,
						TT.NAME AS TEAM_TYPE_NAME
				 FROM TEAM_TYPE TT;
END;

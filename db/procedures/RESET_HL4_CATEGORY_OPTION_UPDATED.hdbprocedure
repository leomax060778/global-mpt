PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::RESET_HL4_CATEGORY_OPTION_UPDATED" (
		IN in_id BIGINT
		,IN in_user_id BIGINT
		,OUT out_result INTEGER
	) 
	LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
   AS
BEGIN
	UPDATE "MKTG_PLANNING_TOOL"."HL4_CATEGORY_OPTION" SET 
	UPDATED = 0
	,modified_user_id = in_user_id
	,modified_date_tz = CURRENT_TIMESTAMP
	WHERE HL4_ID = in_id;
	
	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;

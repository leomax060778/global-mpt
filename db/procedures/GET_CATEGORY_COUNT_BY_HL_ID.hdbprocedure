PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_CATEGORY_COUNT_BY_HL_ID"(
		IN in_hl_id BIGINT, 
		OUT out_hl_category INT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	SELECT COUNT(category_id)
		INTO out_hl_category
		FROM category
		WHERE hierarchy_level_id = in_hl_id
			AND deleted = 0
			AND enabled = 1;
END;

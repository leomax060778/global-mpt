PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_EXPECTED_OUTCOME_BY_NAME"(
		IN IN_NAME NVARCHAR(255), 
		OUT out_result TABLE (
			EXPECTED_OUTCOME_ID BIGINT,
			NAME NVARCHAR(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	out_result = SELECT EXPECTED_OUTCOME_ID, 
				NAME
			FROM EXPECTED_OUTCOME
			WHERE upper(name) = upper(IN_NAME)
				AND deleted = 0
				AND enabled = 1;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL2_USER_BY_ID"(
		IN delHl2User TABLE (
			in_hl2_id BIGINT,
			in_user_id BIGINT
		), 
		OUT out_result BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM HL2_USER
	WHERE ((HL2_USER.HL2_ID, HL2_USER.USER_ID) IN (SELECT in_hl2_id, 
				in_user_id
			FROM :delHl2User));
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_MASS_HL6_CRM_BINDING" (
	IN in_hl6_ids table(
	hl6_id bigint),
	OUT out_result INTEGER
)  
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	AS
BEGIN
 	DELETE FROM "MKTG_PLANNING_TOOL"."HL6_CRM_BINDING" hcb
	WHERE hcb.hl6_id in (SELECT hl6_id FROM :in_hl6_ids);
	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;

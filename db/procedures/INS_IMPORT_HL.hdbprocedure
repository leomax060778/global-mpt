PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::INS_IMPORT_HL" (
	in in_description nvarchar(255),
	IN in_user_id BIGINT,
	OUT OUT_IMPORT_ID INTEGER
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	AS
BEGIN

    INSERT INTO IMPORT_HL(description,created_user_id)
    VALUES (in_description, in_user_id);
	SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO OUT_IMPORT_ID FROM "MKTG_PLANNING_TOOL"."IMPORT_HL";
END;

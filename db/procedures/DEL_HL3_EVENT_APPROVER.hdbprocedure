PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL3_EVENT_APPROVER" (
	IN IN_HL3_ID BIGINT
	,OUT out_result INTEGER
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN
	
	DELETE FROM HL3_EVENT_APPROVER
	WHERE HL3_ID = IN_HL3_ID;
	
	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;
PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_INTERLOCK_ENTITY"(
		OUT out_interlock_entity TABLE (
			INTERLOCK_ENTITY_ID BIGINT,
			NAME NVARCHAR(100)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	out_interlock_entity = SELECT interlock_entity_id, 
				name
			FROM INTERLOCK_ENTITY
			WHERE enabled = 1
				AND deleted = 0
			ORDER BY NAME ASC;
END;

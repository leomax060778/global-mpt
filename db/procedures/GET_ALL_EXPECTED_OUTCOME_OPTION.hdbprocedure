PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_EXPECTED_OUTCOME_OPTION"(
		OUT out_result TABLE (
			expected_outcome_option_id BIGINT,
			name NVARCHAR(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT expected_outcome_option_id, 
				name
			FROM EXPECTED_OUTCOME_OPTION
			WHERE deleted = 0
				AND enabled = 1
			ORDER BY upper(name) ASC;
END;

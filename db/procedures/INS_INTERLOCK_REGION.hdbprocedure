PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::INS_INTERLOCK_REGION" ( 
	IN in_organization_id bigint,
	IN in_interlock_request_id bigint,
	IN in_created_user_id bigint,
	IN in_organization_id_from bigint,
	OUT out_interlock_region_id bigint
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN
	INSERT INTO "MKTG_PLANNING_TOOL"."INTERLOCK_REQUEST_REGION" (region_id,interlock_request_id,created_user_id, region_id_from)
	VALUES (in_organization_id,in_interlock_request_id,in_created_user_id, in_organization_id_from);

	SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO out_interlock_region_id FROM "MKTG_PLANNING_TOOL"."INTERLOCK_REQUEST_REGION";
END;

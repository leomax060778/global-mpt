PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_EXPECTED_OUTCOME_BY_ID"(
		IN in_expected_outcome_id BIGINT, 
		OUT out_result TABLE (
			expected_outcome_id BIGINT,
			name NVARCHAR(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT expected_outcome_id, 
				name
			FROM EXPECTED_OUTCOME
			WHERE deleted = 0
				AND enabled = 1
				AND expected_outcome_id = in_expected_outcome_id;
END;

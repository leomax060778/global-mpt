PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_SUBREGION_BY_NAME_AND_REGION" (
IN IN_NAME nvarchar(255),
IN IN_REGION_ID INTEGER
,OUT out_result TABLE(
	SUBREGION_NAME NVARCHAR(255),
	REGION_ID INTEGER
))
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN

 out_result = 
		
	SELECT 
	SUBREGION_NAME, REGION_ID
 FROM SUBREGION
 WHERE REGION_ID = IN_REGION_ID AND upper(trim(SUBREGION_NAME)) = upper(trim(IN_NAME));
	
END;

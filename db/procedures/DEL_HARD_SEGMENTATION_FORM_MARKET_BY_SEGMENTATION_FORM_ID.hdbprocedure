PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HARD_SEGMENTATION_FORM_MARKET_BY_SEGMENTATION_FORM_ID" (
	IN in_segmentation_form_id BIGINT,
	OUT out_result INTEGER) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	AS
BEGIN

		DELETE FROM SEGMENTATION_FORM_SEGMENTATION_MARKET
		WHERE segmentation_form_id = in_segmentation_form_id;
		
		SELECT ::ROWCOUNT INTO out_result FROM DUMMY;

END;
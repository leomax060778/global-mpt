PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_CAMPAIGN_SUB_TYPE_BY_CRM_KEY"(
		IN IN_CRM_KEY NVARCHAR(255),
		OUT out_result TABLE (
			CAMPAIGN_SUB_TYPE_ID BIGINT,
			NAME NVARCHAR(255),
			CRM_KEY NVARCHAR(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	out_result = SELECT CAMPAIGN_SUB_TYPE_ID,
				NAME,
				CRM_KEY
			FROM CAMPAIGN_SUB_TYPE
			WHERE CRM_KEY = IN_CRM_KEY
				AND deleted = 0
				AND enabled = 1;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_REGISTRATION_PROCESS" (
    OUT out_result TABLE(
        REGISTRATION_PROCESS_ID BIGINT,
        NAME NVARCHAR(255)
    )
)

LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
READS SQL DATA AS
BEGIN
    out_result = SELECT REGISTRATION_PROCESS_ID,
                        NAME
	             FROM "REGISTRATION_PROCESS"
 	             WHERE  ENABLED = 1
 	                AND DELETED = 0;
END;

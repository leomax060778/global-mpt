PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_USER_SESSION_TOKEN"(
		IN in_user_id BIGINT, 
		OUT out_result INTEGER
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM "MKTG_PLANNING_TOOL"."USER_SESSION_TOKEN" AS user_session_token
	WHERE user_session_token.user_id = in_user_id;
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_ROLE"(
		IN in_role_id BIGINT, 
		IN in_user_id BIGINT, 
		OUT out_result INTEGER
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM "MKTG_PLANNING_TOOL"."ROLE" AS role
	WHERE role.role_id = in_role_id;
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

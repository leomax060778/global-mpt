                                           ON HCP.allocation_category_option_level_id =
                                              ACOL.allocation_category_option_level_id
                                   LEFT JOIN hl6_category HC
                                          ON HC.hl6_category_id = HCP.hl6_category_id
                                   LEFT JOIN allocation_category C

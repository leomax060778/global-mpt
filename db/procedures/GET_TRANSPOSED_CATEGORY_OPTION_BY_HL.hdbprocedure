                                           0  AS OPTION_PERCENTAGE
                                    FROM   dummy
                                    UNION ALL
                                    SELECT '' AS OPTION_NAME,
                                           0  AS OPTION_PERCENTAGE
                                    FROM   dummy
                                    UNION ALL
                                    SELECT '' AS OPTION_NAME,
                                           0  AS OPTION_PERCENTAGE
                                    FROM   dummy

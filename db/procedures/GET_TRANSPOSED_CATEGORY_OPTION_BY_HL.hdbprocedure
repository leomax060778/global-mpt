    CASE WHEN (V.ROWNUM = 13 )THEN OPTION_PERCENTAGE ELSE '0' END AS ZZIND_PERCENTAGE3,
    CASE WHEN (V.ROWNUM = 14 )THEN OPTION_NAME ELSE '0' END AS INDUSTRY4,
    CASE WHEN (V.ROWNUM = 14 )THEN OPTION_PERCENTAGE ELSE '0' END AS ZZIND_PERCENTAGE4,
    CASE WHEN (V.ROWNUM = 15 )THEN OPTION_NAME ELSE '0' END AS INDUSTRY5,
    CASE WHEN (V.ROWNUM = 15 )THEN OPTION_PERCENTAGE ELSE '0' END AS ZZIND_PERCENTAGE5,
    CASE WHEN (V.ROWNUM = 16 )THEN OPTION_NAME ELSE '0' END AS SOLUTION1,
    CASE WHEN (V.ROWNUM = 16 )THEN OPTION_PERCENTAGE ELSE '0' END AS ZZSOL_PERCENTAGE1,
    CASE WHEN (V.ROWNUM = 17 )THEN OPTION_NAME ELSE '0' END AS SOLUTION2,
    CASE WHEN (V.ROWNUM = 17 )THEN OPTION_PERCENTAGE ELSE '0' END AS ZZSOL_PERCENTAGE2,
    CASE WHEN (V.ROWNUM = 18 )THEN OPTION_NAME ELSE '0' END AS SOLUTION3,

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL1_USER_BY_ID"(
		IN arrDelHl1User TABLE (
			in_hl1_id BIGINT,
			in_user_id BIGINT
		), 
		OUT out_result BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM HL1_USER
	WHERE ((HL1_USER.HL1_ID, HL1_USER.USER_ID) IN (SELECT in_hl1_id, 
				in_user_id
			FROM :arrDelHl1User));
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

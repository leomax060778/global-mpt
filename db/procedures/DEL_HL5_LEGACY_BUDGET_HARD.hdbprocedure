PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL5_LEGACY_BUDGET_HARD"
(
	IN in_hl5_legacy_id BIGINT,
	OUT out_result BIGINT
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN

	DELETE FROM HL5_LEGACY_BUDGET
    WHERE HL5_LEGACY_ID = in_hl5_legacy_id;
	
	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;
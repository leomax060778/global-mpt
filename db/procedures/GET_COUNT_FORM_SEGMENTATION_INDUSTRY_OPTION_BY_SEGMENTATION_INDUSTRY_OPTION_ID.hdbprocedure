PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_COUNT_FORM_SEGMENTATION_INDUSTRY_OPTION_BY_SEGMENTATION_INDUSTRY_OPTION_ID"(
		IN in_segmentation_industry_option_id BIGINT, 
		OUT out_result BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	SELECT coalesce(
				COUNT(t.SEGMENTATION_FORM_SEGMENTATION_INDUSTRY_OPTION_ID), 
				0
			)
		INTO out_result
		FROM SEGMENTATION_FORM_SEGMENTATION_INDUSTRY_OPTION AS t
		WHERE t.SEGMENTATION_INDUSTRY_OPTION_ID = in_segmentation_industry_option_id
			AND t.deleted = 0
			AND t.enabled = 1;
END;

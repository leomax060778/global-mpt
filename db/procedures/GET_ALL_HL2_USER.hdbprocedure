PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_HL2_USER"(
		IN in_hl2_id BIGINT, 
		OUT out_result TABLE (
			hl2_user_id BIGINT,
			hl2_id BIGINT,
			user_id BIGINT
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT hl2_user_id, 
				hl2_id, 
				user_id
			FROM HL2_USER
			WHERE in_hl2_id = hl2_id;
END;

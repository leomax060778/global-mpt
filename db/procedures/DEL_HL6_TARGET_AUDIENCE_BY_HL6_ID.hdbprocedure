PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_HL6_TARGET_AUDIENCE_BY_HL6_ID"
(
	IN in_hl6_id BIGINT,
	OUT out_result BIGINT
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL" AS
BEGIN
	DELETE FROM HL6_TARGET_AUDIENCE
	WHERE HL6_ID = in_hl6_id;
	
	SELECT ::ROWCOUNT INTO out_result FROM DUMMY;
END;

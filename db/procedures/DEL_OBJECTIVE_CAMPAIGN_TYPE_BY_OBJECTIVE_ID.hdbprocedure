PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_OBJECTIVE_CAMPAIGN_TYPE_BY_OBJECTIVE_ID"(
		IN in_objective_id BIGINT, 
		OUT out_result INTEGER
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM "MKTG_PLANNING_TOOL"."OBJECTIVE_CAMPAIGN_TYPE_SUBTYPE"
	WHERE objective_id = in_objective_id;
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

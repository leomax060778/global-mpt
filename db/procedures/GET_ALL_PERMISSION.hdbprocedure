PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_PERMISSION"(
		OUT out_result TABLE (
			PERMISSION_ID BIGINT,
			NAME NVARCHAR(255)
		)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA
AS
BEGIN
	out_result = SELECT PERMISSION_ID, 
				NAME
			FROM "PERMISSION"
			WHERE ENABLED = 1
				AND DELETED = 0
			ORDER BY NAME ASC;
END;

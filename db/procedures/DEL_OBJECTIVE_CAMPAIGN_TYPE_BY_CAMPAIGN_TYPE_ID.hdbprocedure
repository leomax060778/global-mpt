PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_OBJECTIVE_CAMPAIGN_TYPE_BY_CAMPAIGN_TYPE_ID"(
		IN in_campaign_type_id BIGINT, 
		OUT out_result BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
AS
BEGIN
	DELETE FROM "MKTG_PLANNING_TOOL"."OBJECTIVE_CAMPAIGN_TYPE_SUBTYPE"
	WHERE campaign_type_id = in_campaign_type_id;
	SELECT ::ROWCOUNT
		INTO out_result
		FROM DUMMY;
END;

PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::GET_ALL_COUNTRIES" (
OUT out_result TABLE (
                        country_id INTEGER,
						name NVARCHAR(50),
						crm_key NVARCHAR(3)
					)
 )
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT COUNTRY_ID, NAME, CRM_KEY FROM COUNTRY WHERE DELETED = 0 AND ENABLED = 1 ORDER BY NAME;

END;

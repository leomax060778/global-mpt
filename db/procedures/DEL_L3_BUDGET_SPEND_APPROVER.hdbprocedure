PROCEDURE "MKTG_PLANNING_TOOL"."mktgplanningtool.db.procedures::DEL_L3_BUDGET_SPEND_APPROVER" (
	IN in_l3_id BIGINT
	, IN in_budget_approver_id TABLE (in_user_id BIGINT)
	, OUT out_result BIGINT
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_PLANNING_TOOL"
	AS
BEGIN
		delete from hl3_budget_spend_approver
		where hl3_id = in_l3_id; --and USER_ID not in (select in_user_id from :in_budget_approver_id);

		SELECT ::ROWCOUNT INTO out_result FROM DUMMY;

END;

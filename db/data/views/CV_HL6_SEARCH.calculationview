      </attribute>
      <attribute id="DESCRIPTION" order="12">
        <descriptions defaultDescription="DESCRIPTION"/>
        <keyMapping columnObjectName="Script_View" columnName="DESCRIPTION"/>
      </attribute>
    </attributes>
    <calculatedAttributes/>
    <privateDataFoundation>
      <tableProxies/>
      <joins/>
      <layout>
        <shapes/>
      </layout>
    </privateDataFoundation>
    <baseMeasures>
      <measure id="HL6_ID" order="1" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="HL6_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="HL6_ID"/>
      </measure>
      <measure id="PARENT_ID" order="2" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="PARENT_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="PARENT_ID"/>
      </measure>
      <measure id="REGION_ID" order="5" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="REGION_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="REGION_ID"/>
      </measure>
      <measure id="SUBREGION_ID" order="7" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="SUBREGION_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="SUBREGION_ID"/>
      </measure>
      <measure id="BUDGET_YEAR_ID" order="9" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="BUDGET_YEAR_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="BUDGET_YEAR_ID"/>
      </measure>
      <measure id="STATUS_ID" order="10" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="STATUS_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="STATUS_ID"/>
      </measure>
      <measure id="ENABLED" order="13" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="ENABLED"/>
        <measureMapping columnObjectName="Script_View" columnName="ENABLED"/>
      </measure>
      <measure id="DELETED" order="14" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="DELETED"/>
        <measureMapping columnObjectName="Script_View" columnName="DELETED"/>
      </measure>
      <measure id="IS_LEGACY" order="15" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="IS_LEGACY"/>
        <measureMapping columnObjectName="Script_View" columnName="IS_LEGACY"/>
      </measure>
    </baseMeasures>

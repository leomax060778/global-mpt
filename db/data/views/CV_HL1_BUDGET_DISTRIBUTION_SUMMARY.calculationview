        <keyMapping columnObjectName="Script_View" columnName="HL1_DESCRIPTION"/>
      </attribute>
      <attribute id="VERSION" order="15">
        <descriptions defaultDescription="VERSION"/>
        <keyMapping columnObjectName="Script_View" columnName="VERSION"/>
      </attribute>
      <attribute id="LEVEL" order="16">
        <descriptions defaultDescription="LEVEL"/>
        <keyMapping columnObjectName="Script_View" columnName="LEVEL"/>
      </attribute>
    </attributes>
    <calculatedAttributes/>
    <privateDataFoundation>
      <tableProxies/>
      <joins/>
      <layout>
        <shapes/>
      </layout>
    </privateDataFoundation>
    <baseMeasures>
      <measure id="L1_BUDGET_REMAINING" order="4" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="L1_BUDGET_REMAINING"/>
        <measureMapping columnObjectName="Script_View" columnName="L1_BUDGET_REMAINING"/>
      </measure>
      <measure id="L1_BUDGET_ALLOCATED" order="5" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="L1_BUDGET_REMAINING"/>
        <measureMapping columnObjectName="Script_View" columnName="L1_BUDGET_ALLOCATED"/>
      </measure>
      <measure id="TOTAL_OF_PRIORITIES" order="6" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="TOTAL_OF_PRIORITIES"/>
        <measureMapping columnObjectName="Script_View" columnName="TOTAL_OF_PRIORITIES"/>
      </measure>
      <measure id="BUDGET_YEAR_ID" order="10" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="BUDGET_YEAR_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="BUDGET_YEAR_ID"/>
      </measure>
      <measure id="REGION_ID" order="11" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="REGION_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="REGION_ID"/>
      </measure>
      <measure id="IS_LOCKED" order="12" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="IS_LOCKED"/>
        <measureMapping columnObjectName="Script_View" columnName="IS_LOCKED"/>
      </measure>
      <measure id="TOTAL_L1_BUDGET" order="13" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="TOTAL_L1_BUDGET"/>
        <measureMapping columnObjectName="Script_View" columnName="TOTAL_L1_BUDGET"/>
      </measure>
    </baseMeasures>

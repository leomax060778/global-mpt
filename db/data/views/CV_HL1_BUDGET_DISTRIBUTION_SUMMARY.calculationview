<?xml version="1.0" encoding="UTF-8"?>
<Calculation:scenario xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:Calculation="http://www.sap.com/ndb/BiModelCalculation.ecore" schemaVersion="2.3" id="CV_HL1_BUDGET_DISTRIBUTION_SUMMARY" applyPrivilegeType="ANALYTIC_PRIVILEGE" checkAnalyticPrivileges="true" defaultClient="$$client$$" defaultLanguage="$$language$$" hierarchiesSQLEnabled="false" translationRelevant="true" visibility="reportingEnabled" calculationScenarioType="SCRIPT_BASED" dataCategory="CUBE" enforceSqlExecution="false" executionSemantic="UNDEFINED" scriptParametersCaseSensitive="true">
  <descriptions defaultDescription="CV_HL1_KPI_DATA"/>
  <localVariables/>
  <variableMappings/>
  <informationModelLayout relativeWidthScenario="27"/>
  <dataSources/>
  <calculationViews>
    <calculationView xsi:type="Calculation:SqlScriptView" id="Script_View">
      <descriptions/>
      <viewAttributes>
        <viewAttribute datatype="BIGINT" id="HL1_ID"/>
        <viewAttribute datatype="DECIMAL" id="L1_BUDGET" length="19" scale="2"/>
        <viewAttribute datatype="DECIMAL" id="L1_BUDGET_PERCENTAGE" length="10" scale="2"/>
        <viewAttribute datatype="DECIMAL" id="L1_BUDGET_REMAINING" length="19" scale="2"/>
        <viewAttribute datatype="DECIMAL" id="L1_BUDGET_ALLOCATED" length="19" scale="2"/>
        <viewAttribute datatype="INTEGER" id="TOTAL_OF_PRIORITIES"/>
        <viewAttribute datatype="NVARCHAR" id="L1_ACRONYM" length="255"/>
        <viewAttribute datatype="NVARCHAR" id="BUDGET_YEAR" length="255"/>
        <viewAttribute datatype="NVARCHAR" id="REGION_NAME" length="255"/>
        <viewAttribute datatype="BIGINT" id="BUDGET_YEAR_ID"/>
        <viewAttribute datatype="BIGINT" id="REGION_ID"/>
        <viewAttribute datatype="TINYINT" id="IS_LOCKED"/>
        <viewAttribute datatype="DECIMAL" id="TOTAL_L1_BUDGET" length="19" scale="2"/>
        <viewAttribute datatype="NVARCHAR" id="HL1_DESCRIPTION" length="255"/>
        <viewAttribute datatype="INTEGER" id="VERSION"/>
        <viewAttribute datatype="NVARCHAR" id="LEVEL" length="3"/>
      </viewAttributes>
      <calculatedViewAttributes/>
      <definition>/********* Begin Procedure Script ************/
BEGIN

va_l1 = SELECT HL1.BUDGET_YEAR_ID, SUM(HL1.BUDGET) AS TOTAL_L1_BUDGET
	FROM &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_HL1&quot; HL1
	WHERE HL1.ENABLED = 1
	AND HL1.DELETED = 0
	GROUP BY HL1.BUDGET_YEAR_ID;

va_l2 = SELECT HL2.HL1_ID
	, COUNT(HL2.HL2_ID) AS HL1_TOTAL_PRIORITIES
	, SUM(HL2.HL2_BUDGET_TOTAL) AS HL1_BUDGET_ALLOCATED
	FROM &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_HL2&quot; HL2
	WHERE HL2.ENABLED = 1
	AND HL2.DELETED = 0
	GROUP BY HL2.HL1_ID;

var_out = select hl1.HL1_ID
    , hl1.BUDGET AS L1_BUDGET
    , (hl1.BUDGET * 100 / (CASE WHEN T.TOTAL_L1_BUDGET = 0 THEN 1 ELSE T.TOTAL_L1_BUDGET END)) AS L1_BUDGET_PERCENTAGE
    , (hl1.BUDGET - COALESCE(K.HL1_BUDGET_ALLOCATED,0)) AS L1_BUDGET_REMAINING
    , (
    	COALESCE(K.HL1_BUDGET_ALLOCATED,0) * 100 / (CASE WHEN hl1.BUDGET = 0 THEN 1 ELSE hl1.BUDGET END)
    	) AS L1_BUDGET_ALLOCATED
    , COALESCE(K.HL1_TOTAL_PRIORITIES,0) AS TOTAL_OF_PRIORITIES
     , hl1.ACRONYM AS L1_ACRONYM
     , budgetYear.BUDGET_YEAR
     , IFNULL(region.REGION_NAME,'') AS REGION_NAME
     , hl1.BUDGET_YEAR_ID
     , hl1.REGION_ID
     , (CASE WHEN budgetYear.VERSIONED_END_DATE >= CURRENT_TIMESTAMP THEN 0 ELSE 1 END) AS IS_LOCKED
     , T.TOTAL_L1_BUDGET
     , hl1.DESCRIPTION AS hl1_description
     , hl1.VERSION
     , 'HL1' AS LEVEL
    FROM &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_HL1&quot; hl1
    INNER JOIN :va_l1 T ON T.BUDGET_YEAR_ID = hl1.BUDGET_YEAR_ID
    LEFT JOIN :va_l2 K ON K.HL1_ID = hl1.HL1_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_BUDGET_YEAR&quot; budgetYear
        ON budgetYear.BUDGET_YEAR_ID = hl1.BUDGET_YEAR_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_REGION&quot; region
        ON region.REGION_ID = hl1.REGION_ID
    where hl1.DELETED = 0 AND hl1.ENABLED = 1
    ORDER BY budgetYear.BUDGET_YEAR
    , hl1.ACRONYM;
END /********* End Procedure Script ************/</definition>
    </calculationView>
  </calculationViews>
  <logicalModel id="Script_View">
    <descriptions/>

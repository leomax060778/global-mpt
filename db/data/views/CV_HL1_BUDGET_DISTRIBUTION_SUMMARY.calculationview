<?xml version="1.0" encoding="UTF-8"?>
<Calculation:scenario xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:Calculation="http://www.sap.com/ndb/BiModelCalculation.ecore" schemaVersion="2.3" id="CV_HL1_BUDGET_DISTRIBUTION_SUMMARY" applyPrivilegeType="ANALYTIC_PRIVILEGE" checkAnalyticPrivileges="true" defaultClient="$$client$$" defaultLanguage="$$language$$" hierarchiesSQLEnabled="false" translationRelevant="true" visibility="reportingEnabled" calculationScenarioType="SCRIPT_BASED" dataCategory="CUBE" enforceSqlExecution="false" executionSemantic="UNDEFINED" scriptParametersCaseSensitive="true">
  <descriptions defaultDescription="CV_HL1_KPI_DATA"/>
  <localVariables/>
  <variableMappings/>
  <informationModelLayout relativeWidthScenario="27"/>
  <dataSources/>
  <calculationViews>
    <calculationView xsi:type="Calculation:SqlScriptView" id="Script_View">
      <descriptions/>
      <viewAttributes>
        <viewAttribute datatype="BIGINT" id="HL1_ID"/>
        <viewAttribute datatype="DECIMAL" id="L1_BUDGET" length="10" scale="2"/>
        <viewAttribute datatype="DECIMAL" id="L1_BUDGET_PERCENTAGE" length="10" scale="2"/>
        <viewAttribute datatype="DECIMAL" id="L1_BUDGET_REMAINING" length="10" scale="2"/>
        <viewAttribute datatype="DECIMAL" id="L1_BUDGET_ALLOCATED" length="10" scale="2"/>
        <viewAttribute datatype="INTEGER" id="TOTAL_OF_PRIORITIES"/>
        <viewAttribute datatype="NVARCHAR" id="L1_ACRONYM" length="255"/>
        <viewAttribute datatype="NVARCHAR" id="BUDGET_YEAR" length="255"/>
        <viewAttribute datatype="NVARCHAR" id="REGION_NAME" length="255"/>
        <viewAttribute datatype="NVARCHAR" id="MARKET_UNIT_NAME" length="255"/>
        <viewAttribute datatype="BIGINT" id="BUDGET_YEAR_ID"/>
        <viewAttribute datatype="BIGINT" id="REGION_ID"/>
        <viewAttribute datatype="BIGINT" id="SUBREGION_ID"/>
        <viewAttribute datatype="TINYINT" id="IS_LOCKED"/>
        <viewAttribute datatype="NVARCHAR" id="CATEGORY_NAME" length="255"/>
        <viewAttribute datatype="NVARCHAR" id="OPTION_NAME" length="255"/>
        <viewAttribute datatype="DECIMAL" id="BUDGET_DISTRIBUTION_PERCENTAGE" length="19" scale="2"/>
        <viewAttribute datatype="DECIMAL" id="BUDGET_DISTRIBUTION_AMOUNT" length="19" scale="2"/>
        <viewAttribute datatype="DECIMAL" id="TOTAL_L1_BUDGET" length="19" scale="2"/>
        <viewAttribute datatype="NVARCHAR" id="HL1_DESCRIPTION" length="255"/>
        <viewAttribute datatype="INTEGER" id="VERSION"/>
        <viewAttribute datatype="DECIMAL" id="SUB_AMOUNT" length="19" scale="2"/>
        <viewAttribute datatype="BIGINT" id="ALLOCATION_OPTION_ID"/>
      </viewAttributes>
      <calculatedViewAttributes/>
      <definition>/********* Begin Procedure Script ************/
BEGIN

va_l1 = SELECT HL1.BUDGET_YEAR_ID, SUM(HL1.BUDGET) AS TOTAL_L1_BUDGET
	FROM &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_HL1&quot; HL1
	WHERE HL1.ENABLED = 1
	AND HL1.DELETED = 0
	GROUP BY HL1.BUDGET_YEAR_ID;

va_l2 = SELECT HL2.HL1_ID
	, COUNT(HL2.HL2_ID) AS HL1_TOTAL_PRIORITIES
	, SUM(HL2.HL2_BUDGET_TOTAL) AS HL1_BUDGET_ALLOCATED
	FROM &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_HL2&quot; HL2
	WHERE HL2.ENABLED = 1
	AND HL2.DELETED = 0
	GROUP BY HL2.HL1_ID;
	
va_l3 = SELECT HL2.HL1_ID, SUM(HL2_CO.AMOUNT * HL2.HL2_BUDGET_TOTAL / 100) AS SUB_AMOUNT, ACOL.ALLOCATION_OPTION_ID, AO.NAME
 FROM &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_HL2_CATEGORY_OPTION&quot; HL2_CO    	
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_ALLOCATION_CATEGORY_OPTION_LEVEL&quot; ACOL
    	ON HL2_CO.ALLOCATION_CATEGORY_OPTION_LEVEL_ID = ACOL.ALLOCATION_CATEGORY_OPTION_LEVEL_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_ALLOCATION_CATEGORY&quot; AC
    	ON AC.ALLOCATION_CATEGORY_ID = ACOL.ALLOCATION_CATEGORY_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_ALLOCATION_OPTION&quot; AO
    	ON AO.ALLOCATION_OPTION_ID = ACOL.ALLOCATION_OPTION_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_HL2&quot; HL2 ON HL2.HL2_ID = HL2_CO.HL2_ID
    GROUP BY HL2.HL1_ID,ACOL.ALLOCATION_OPTION_ID,AO.NAME;
 


var_out = select hl1.HL1_ID
    , hl1.BUDGET AS L1_BUDGET
    , (hl1.BUDGET * 100 / T.TOTAL_L1_BUDGET) AS L1_BUDGET_PERCENTAGE
    , (hl1.BUDGET - COALESCE(K.HL1_BUDGET_ALLOCATED,0)) AS L1_BUDGET_REMAINING
    , (
    	COALESCE(K.HL1_BUDGET_ALLOCATED,0) * 100 / (CASE WHEN hl1.BUDGET = 0 THEN 1 ELSE hl1.BUDGET END)
    	) AS L1_BUDGET_ALLOCATED
    , COALESCE(K.HL1_TOTAL_PRIORITIES,0) AS TOTAL_OF_PRIORITIES
     , hl1.ACRONYM AS L1_ACRONYM
     , budgetYear.BUDGET_YEAR
     , IFNULL(region.REGION_NAME,'') AS REGION_NAME
     , IFNULL(subregion.SUBREGION_NAME,'') AS MARKET_UNIT_NAME
     , hl1.BUDGET_YEAR_ID
     , hl1.REGION_ID
     , hl1.SUBREGION_ID
     , (CASE WHEN budgetYear.VERSIONED_END_DATE >= CURRENT_TIMESTAMP THEN 0 ELSE 1 END) AS IS_LOCKED
     , AC.NAME AS CATEGORY_NAME
     , AO.NAME AS OPTION_NAME
     , HL1_CO.AMOUNT AS BUDGET_DISTRIBUTION_PERCENTAGE
     , (hl1.BUDGET * HL1_CO.AMOUNT / 100) AS BUDGET_DISTRIBUTION_AMOUNT
     , T.TOTAL_L1_BUDGET
     , hl1.DESCRIPTION AS hl1_description
     , hl1.VERSION
     , G.SUB_AMOUNT AS SUB_AMOUNT
     , G.ALLOCATION_OPTION_ID
	--, 0 AS SUB_AMOUNT
	--,0 AS ALLOCATION_OPTION_ID
    FROM &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_HL1&quot; hl1
    INNER JOIN :va_l1 T ON T.BUDGET_YEAR_ID = hl1.BUDGET_YEAR_ID
    LEFT JOIN :va_l2 K ON K.HL1_ID = hl1.HL1_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_HL1_CATEGORY_OPTION&quot; HL1_CO
    	ON HL1_CO.HL1_ID = hl1.HL1_ID and HL1_CO.AMOUNT > 0 AND HL1_CO.AMOUNT IS NOT NULL
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_ALLOCATION_CATEGORY_OPTION_LEVEL&quot; ACOL
    	ON HL1_CO.ALLOCATION_CATEGORY_OPTION_LEVEL_ID = ACOL.ALLOCATION_CATEGORY_OPTION_LEVEL_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_ALLOCATION_CATEGORY&quot; AC
    	ON AC.ALLOCATION_CATEGORY_ID = ACOL.ALLOCATION_CATEGORY_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_ALLOCATION_OPTION&quot; AO
    	ON AO.ALLOCATION_OPTION_ID = ACOL.ALLOCATION_OPTION_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_BUDGET_YEAR&quot; budgetYear
        ON budgetYear.BUDGET_YEAR_ID = hl1.BUDGET_YEAR_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_REGION&quot; region
        ON region.REGION_ID = hl1.REGION_ID
    LEFT JOIN &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_SUBREGION&quot; subregion
        ON subregion.SUBREGION_ID = hl1.SUBREGION_ID
    LEFT JOIN :va_l3 G ON (G.HL1_ID = hl1.HL1_ID  AND G.ALLOCATION_OPTION_ID = AO.ALLOCATION_OPTION_ID)
    where hl1.DELETED = 0 AND hl1.ENABLED = 1
    ORDER BY budgetYear.BUDGET_YEAR
    , hl1.ACRONYM;
END /********* End Procedure Script ************/</definition>
    </calculationView>
  </calculationViews>
  <logicalModel id="Script_View">
    <descriptions/>
    <attributes>
      <attribute id="HL1_ID" order="1">

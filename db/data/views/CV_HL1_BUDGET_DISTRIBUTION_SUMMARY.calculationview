      </attribute>
      <attribute id="HL1_DESCRIPTION" order="20">
        <descriptions defaultDescription="HL1_DESCRIPTION"/>
        <keyMapping columnObjectName="Script_View" columnName="HL1_DESCRIPTION"/>
      </attribute>
      <attribute id="VERSION" order="21">
        <descriptions defaultDescription="VERSION"/>
        <keyMapping columnObjectName="Script_View" columnName="VERSION"/>
      </attribute>
    </attributes>
    <calculatedAttributes/>
    <privateDataFoundation>
      <tableProxies/>
      <joins/>
      <layout>
        <shapes/>
      </layout>
    </privateDataFoundation>
    <baseMeasures>
      <measure id="L1_BUDGET_REMAINING" order="4" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="L1_BUDGET_REMAINING"/>
        <measureMapping columnObjectName="Script_View" columnName="L1_BUDGET_REMAINING"/>
      </measure>
      <measure id="L1_BUDGET_ALLOCATED" order="5" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="L1_BUDGET_REMAINING"/>
        <measureMapping columnObjectName="Script_View" columnName="L1_BUDGET_ALLOCATED"/>
      </measure>
      <measure id="TOTAL_OF_PRIORITIES" order="6" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="TOTAL_OF_PRIORITIES"/>
        <measureMapping columnObjectName="Script_View" columnName="TOTAL_OF_PRIORITIES"/>
      </measure>
      <measure id="BUDGET_YEAR_ID" order="11" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="BUDGET_YEAR_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="BUDGET_YEAR_ID"/>
      </measure>
      <measure id="REGION_ID" order="12" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="REGION_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="REGION_ID"/>
      </measure>
      <measure id="SUBREGION_ID" order="13" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="SUBREGION_ID"/>
        <measureMapping columnObjectName="Script_View" columnName="SUBREGION_ID"/>
      </measure>
      <measure id="IS_LOCKED" order="14" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="IS_LOCKED"/>
        <measureMapping columnObjectName="Script_View" columnName="IS_LOCKED"/>
      </measure>
      <measure id="BUDGET_DISTRIBUTION_PERCENTAGE" order="17" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="BUDGET_DISTRIBUTION_PERCENTAGE"/>
        <measureMapping columnObjectName="Script_View" columnName="BUDGET_DISTRIBUTION_PERCENTAGE"/>
      </measure>
      <measure id="BUDGET_DISTRIBUTION_AMOUNT" order="18" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="BUDGET_DISTRIBUTION_AMOUNT"/>
        <measureMapping columnObjectName="Script_View" columnName="BUDGET_DISTRIBUTION_AMOUNT"/>
      </measure>
      <measure id="TOTAL_L1_BUDGET" order="19" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="TOTAL_L1_BUDGET"/>

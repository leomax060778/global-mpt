      <calculatedViewAttributes/>
      <definition>/********* Begin Procedure Script ************/
BEGIN
var_out =
SELECT
ATHS.HL4_ID,
ATHS.HL3_ID AS PARENT_ID,
ATHS.ACRONYM_2 AS ACRONYM, --ACRONYM_2 belongs to HL4
ATHS.PATH,
ATHS.REGION_ID,
ATHS.REGION_NAME,
ATHS.SUBREGION_ID,
ATHS.SUBREGION_NAME,
ATHS.BUDGET_YEAR_ID,
ATHS.HL4_STATUS_DETAIL_ID AS STATUS_ID,
ATHS.DETAIL AS STATUS,
ATHS.HL4_CRM_DESCRIPTION AS DESCRIPTION,
ATHS.ENABLED,
ATHS.DELETED
FROM &quot;_SYS_BIC&quot;.&quot;mktgplanningtool.db.data.views/AT_HL4_SEARCH&quot; ATHS
WHERE
ATHS.DELETED = 0 AND ATHS.ENABLED = 1;


END
/********* End Procedure Script ************/</definition>
    </calculationView>
  </calculationViews>
